+def vllm__module_executor__models__gpt_neox__GPTNeoXAttention__forward(
+    self,
+    position_ids: torch.Tensor,
+    hidden_states: torch.Tensor,
+) -> torch.Tensor:
+    qkv, _ = self.query_key_value(hidden_states)
