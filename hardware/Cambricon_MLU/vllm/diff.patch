+        attn_bias = BlockDiagonalMask.from_seqlens(q_seqlen=seqlens,
+                                                    kv_seqlen=None,
+                                                    device=q.device)
+
+        context_layer = xops.memory_efficient_attention_forward(
+            q, k, v, attn_bias=attn_bias, p=0, scale=None)
