+            end_marker = torch.mlu.Event(enable_timing=True)
+            end_marker.record()
+            self.time_markers = (start_marker, end_marker)
+
+        return embedding_result
+        '''
+        ==================
+        End of MLU Hijack
+        ==================
+        '''
