+    cla_coeffient = bcfg["cla_coeffient"]
+    kv_cache = cla_coeffient * batch * max(head_num_kv, tp_num) * head_size * (input_seq_len + output_seq_len / 2) * 2 * layer_num * kv_cache_byte_width
+    return kv_cache
