+#         --wandb-exp-name ${WANDB_NAME:-"Mixtral_8x7B"} 
+#     )
+# fi
+
+cmd="torchrun ${DISTRIBUTED_ARGS[@]} $WORK_HOME/pretrain_mixtral.py \
+        ${MODEL_ARGS[@]} \
+        ${TRAINING_ARGS[@]} \
+        ${REGULARIZATION_ARGS[@]}
+        ${LEARNING_RATE_ARGS[@]} \
