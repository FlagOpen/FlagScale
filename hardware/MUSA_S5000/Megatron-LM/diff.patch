+    export EPX_SESSION="a0515990-ffbf-11ef-8a45-6fde377b8f7a"
+    export EPX_GROUP_RANK=1
+
+    export USE_EPX=$USE_EPX
+
+    ROOT_PATH=${ROOT_PATH:-"/root/workspace"}
