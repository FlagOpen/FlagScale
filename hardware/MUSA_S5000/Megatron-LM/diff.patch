+    plt.title("lr compare")
+    plt.xlabel("iterations")
+    plt.ylabel("lr")
+    plt.legend(["cuda", "musa"], loc='upper right')
+
+    plt.show()
+    print(f"diff: {np.mean(diff_ratios):.3f}(+-{np.var(diff_ratios):.3f})%")
+
