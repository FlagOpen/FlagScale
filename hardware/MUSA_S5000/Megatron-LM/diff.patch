+usage() {
+    echo "Usage: $0 [OPTIONS]"
+    echo "Options:"
+    echo "  --data_format      Set data format (default: fp8)."
+    echo "  --dataset_dir      Set dataset directory(required)."
+    echo "  --root_path        Set epx root path (default: /root/workspace)."
