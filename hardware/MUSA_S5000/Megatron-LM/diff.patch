+    CUDA_FILE = "2024-05-23_10:40:16/tp1_pp1_dp2_mbs1_numbs1_gbs2_gpus2.log.0.172.31.208.6" # 10b, 6 layers
+    # MUSA_FILE = "2024-05-21_16:45:41/tp1_pp1_dp4_mbs1_numbs1_gbs4_gpus4.log.0.10.70.147.165"  # done
