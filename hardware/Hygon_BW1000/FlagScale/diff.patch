+        return PackedSeqParams(
+            cu_seqlens_q=cu_seqlens,
+            cu_seqlens_kv=cu_seqlens,
+            qkv_format='thd',
+            max_seqlen_q=max_seqlen_q,
+            max_seqlen_kv=max_seqlen_q
+        )
