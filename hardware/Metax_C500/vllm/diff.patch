+            }
+        } else {
+            for (int y = 0; y < N; y++) {
+                for (int x = tidRow; x < LoopNum; x += BlockDimX) {
+                    bsm_b_ptr[x + tidCol * LoopNum + y * LoopNum * thread_groups] = srcB[splitKOffset + i + tidCol * LoopNum + x + y * k_stride];
+                }
+            }
+        }
+
+        __syncthreads();
+
+        //Load zero and scale from bsm
+        if (m_index < this_group_elements) {
