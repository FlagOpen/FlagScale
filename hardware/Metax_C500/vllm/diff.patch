+    } else {
+        v2f a0;
+        int p0 = p & 0x0f0f0f0f;
+        CVT_B0TOF32(p0, a0.x);
+        CVT_B2TOF32(p0, a0.y);
+        a0 = __builtin_mxc_pk_fma_f32(a0, scale, scale_zero);
