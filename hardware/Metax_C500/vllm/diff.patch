+    uint64_t max_exponent = (uint64_t(1) << exponent) - 2;
+    if (nan_repr == NAN_EXTD_RANGE_MAX_MIN || nan_repr == NAN_NONE) {
+      //TORCH_CHECK(exponent < 11,
+      //            "Cannot represent max/min as a double for type ", str());
+      max_exponent += 1;
+    }
