+        } else {
+            swap_b_cache(0);
+            swap_b_cache(1);
+            swap_b_cache(2);
+            swap_b_cache(3);
+            dequant(0); //dequant b0
+            sts_a(); // reorder data_a from reg(gvm resouce) to bsm
+            ldg_b(k_idx, 1);
+            dequant(1); //dequant b0
+            dequant(2); //dequant b0
+            dequant(3); //dequant b0
