+        if constexpr(w_type_id == vllm::kU4.id() || w_type_id == vllm::kU4B8.id()) {
+            swap_b_cache(0);
+            dequant(0); //dequant b0
+            sts_a(); // reorder data_a from reg(gvm resouce) to bsm
+            swap_b_cache(1);
