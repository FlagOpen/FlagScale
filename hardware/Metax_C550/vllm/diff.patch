+            quant_packed_type A[4];
+            const int packed_a_stride = srcAStride / PACK_RATIO;
+            int src_a_offset = (loop_index + i + tidCol * LoopNum) * srcAStride / PACK_RATIO + m_offset_a + m_per_thread * tidRow / PACK_RATIO;
