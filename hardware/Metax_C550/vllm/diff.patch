+                src_a_offset += packed_a_stride;
+                A[1] = srcA[src_a_offset];
+                DEQUANT_FMA(A[2], local_b[2])
+                DEQUANT_FMA(A[3], local_b[3])
+            }
+            src_a_offset += packed_a_stride;
+            A[2] = srcA[src_a_offset];
+            src_a_offset += packed_a_stride;
+            A[3] = srcA[src_a_offset];
