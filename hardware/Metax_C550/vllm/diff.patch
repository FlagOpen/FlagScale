+        int pred_k = tile_k_idx * COMPACT_FACTOR < k;
+        int pred_n = tile_n_idx * COMPACT_FACTOR + i < n;
+        if (pred_k && pred_n) {
+            *(output + gvm_addr_offset) = gptq_data[i];
+        } else {
+            *(output + gvm_addr_offset) = 0x00000000;
+        }
+    }
+}
